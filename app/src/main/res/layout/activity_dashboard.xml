<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main"
    android:background="#FFFFFF"
    android:clipChildren="false"
    android:clipToPadding="false"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".dashboard">

    <ScrollView
        android:id="@+id/rtjz82m2nwo"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="@drawable/cr30b100425"
        android:clipChildren="false"
        android:clipToPadding="false"
        android:paddingTop="20dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/rg5hz5bwon8d"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="18dp"
                android:clipChildren="false"
                android:clipToPadding="false">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:clipChildren="false"
                    android:clipToPadding="false"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/rol3oa70yzjm"
                        android:layout_width="155dp"
                        android:layout_height="222dp"
                        android:layout_marginTop="1dp"
                        android:orientation="vertical">


                        <LinearLayout
                            android:id="@+id/rs795483tfsk"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="138dp"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <ImageView
                                android:id="@+id/r63ky6xgnsb"
                                android:layout_width="53dp"
                                android:layout_height="39dp"
                                android:layout_marginHorizontal="6dp"

                                android:scaleType="fitXY"
                                android:src="@drawable/screenshot_"
                                app:shapeAppearanceOverlay="@style/cr70" />


                            <TextView
                                android:id="@+id/rqtb95bi74ol"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="S.GATE"
                                android:textColor="#FFFFFF"
                                android:textSize="24sp"
                                android:textStyle="bold" />
                        </LinearLayout>


                        <TextView
                            android:id="@+id/rcv4ah79cvrl"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginHorizontal="6dp"
                            android:text="Last Access : 2:30 PM"
                            android:textColor="#FFFFFF"
                            android:textSize="14sp" />

                        <TextView
                            android:id="@+id/email"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginHorizontal="6dp"
                            android:text="Santosh@gmail.com"
                            android:textColor="#FFFFFF"
                            android:textSize="14sp" />

                        <TextView
                            android:id="@+id/r17odbvd2hj6"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginHorizontal="10dp"
                            android:layout_marginTop="-140dp"
                            android:layout_marginEnd="5dp"
                            android:layout_marginBottom="18dp"
                            android:text="Namaskar, Santosh"
                            android:textColor="#FFFFFF"
                            android:textSize="29sp"
                            android:textStyle="bold" />

                    </LinearLayout>

                    <View
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_weight="1" />

                    <RelativeLayout
                        android:id="@+id/rzso8fl7xmt"
                        android:layout_width="233dp"
                        android:layout_height="wrap_content"
                        android:clipChildren="false"
                        android:clipToPadding="false">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:clipChildren="false"
                            android:clipToPadding="false"
                            android:orientation="vertical">

                            <RelativeLayout
                                android:id="@+id/rh8r5yqog06c_rl"
                                android:layout_width="match_parent"
                                android:layout_height="221dp">

                                <ImageView
                                    android:id="@+id/rh8r5yqog06c"
                                    android:layout_width="match_parent"
                                    android:layout_height="221dp"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/ellipse" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:orientation="vertical"
                                    android:paddingVertical="8dp"
                                    android:paddingRight="1dp">

                                    <ImageView
                                        android:id="@+id/rk1k00uptik"
                                        android:layout_width="@dimen/_130sdp"
                                        android:layout_height="@dimen/_25sdp"
                                        android:layout_marginStart="40dp"
                                        android:layout_marginBottom="41dp"
                                        android:scaleType="fitXY"
                                        android:src="@drawable/uulite" />

                                    <LinearLayout
                                        android:id="@+id/rsoey99yh1ss"
                                        android:layout_width="130dp"
                                        android:layout_height="100dp"
                                        android:layout_marginStart="100dp"
                                        android:orientation="vertical">

                                        <ImageView
                                            android:id="@+id/hola"
                                            android:layout_width="100dp"
                                            android:layout_height="100dp"
                                            android:layout_marginStart="1dp"
                                            android:layout_marginBottom="20dp"
                                            android:scaleType="fitXY"
                                            android:src="@drawable/rounds" />
                                    </LinearLayout>
                                </LinearLayout>
                            </RelativeLayout>
                        </LinearLayout>

                    </RelativeLayout>
                </LinearLayout>

                <TextView
                    android:id="@+id/rppol4hf65nn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="59dp"
                    android:layout_marginTop="28dp"
                    android:text="Security At your Fingertips"
                    android:textColor="#FFFFFF"
                    android:textSize="8sp" />
            </RelativeLayout>

            <LinearLayout
                android:id="@+id/r1w3zsm2arzm"
                android:layout_width="match_parent"
                android:layout_height="683dp"
                android:background="@drawable/cr50bffffff"
                android:clipChildren="false"
                android:clipToPadding="false"
                android:orientation="vertical"
                android:paddingTop="30dp"
                android:paddingBottom="204dp">

                <ImageView
                    android:id="@+id/rc3iy3v6pn7i"
                    android:layout_width="match_parent"
                    android:layout_height="274dp"
                    android:layout_marginHorizontal="70dp"
                    android:layout_marginBottom="13dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/qr" />

                <TextView
                    android:id="@+id/r3aybn1kgehg"
                    android:layout_width="305dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="40dp"
                    android:layout_marginBottom="28dp"
                    android:fontFamily="@font/kavoon"
                    android:text="SCAN QR CODE TO LOGIN"
                    android:textColor="#4017E3"
                    android:textSize="25sp" />

                <LinearLayout
                    android:id="@+id/rqfucxn0y5co"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginHorizontal="14dp"
                    android:background="@drawable/cr30lr4054017e39811ba"
                    android:clipChildren="false"
                    android:clipToPadding="false"
                    android:elevation="10dp"
                    android:orientation="vertical"
                    android:paddingHorizontal="137dp"
                    android:paddingVertical="21dp">

                    <TextView
                        android:id="@+id/rvhuoh3m176"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="SCAN NOW"
                        android:textColor="#FFFFFF"
                        android:textSize="19sp" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>